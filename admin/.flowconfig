[ignore]
<PROJECT_ROOT>/.git/
<PROJECT_ROOT>/.install/
<PROJECT_ROOT>/config/
<PROJECT_ROOT>/scripts/
<PROJECT_ROOT>/server/
<PROJECT_ROOT>/website/
.*/node_modules/react-apollo/
.*/node_modules/bcryptjs/src/bower.json
.*/node_modules/config-chain/test/*
.*/node_modules/npm/node_modules/config-chain/test/*
.*/node_modules/npm/test/fixtures/config/*
.*/node_modules/eslint-plugin-jsx-a11y/src/.*
.*/node_modules/.*\.json$

[options]
module.name_mapper='preset-client' -> 'empty/object'
module.name_mapper='.*\(.graphql\)' -> 'empty/object'
module.system.node.resolve_dirname=<PROJECT_ROOT>/../preset/src
module.system.node.resolve_dirname=src
module.system.node.resolve_dirname=node_modules
esproposal.export_star_as=enable
