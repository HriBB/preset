{
  "name": "preset-cms",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@lingui/babel-preset-react": "^2.0.5",
    "enzyme": "^3.3.0",
    "flow-bin": "0.67.1",
    "prettier-eslint-cli": "4.7.1"
  },
  "licence": "",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-flow": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@lingui/react": "^2.0.5",
    "apollo-cache-inmemory": "1.1.10",
    "apollo-client": "2.2.6",
    "apollo-link": "1.2.1",
    "apollo-link-context": "1.0.7",
    "apollo-link-http": "1.5.3",
    "apollo-upload-client": "8.0.0",
    "autoprefixer": "8.1.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^8.0.0-beta",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "classnames": "^2.2.5",
    "css-loader": "0.28.10",
    "dotenv": "5.0.1",
    "dotenv-expand": "4.2.0",
    "eslint": "4.18.2",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fs-extra": "5.0.0",
    "graphql": "^0.13.1",
    "graphql-tag": "2.8.0",
    "html-webpack-plugin": "3.0.6",
    "jest": "22.4.2",
    "jss": "^9.8.0",
    "jss-extend": "^6.2.0",
    "jss-preset-default": "^4.3.0",
    "material-ui": "^1.0.0-beta.32",
    "material-ui-icons": "1.0.0-beta.36",
    "object-assign": "4.1.1",
    "parse-key-value": "^1.0.0",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-apollo": "2.1.0-beta.3",
    "react-dev-utils": "^5.0.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "4.0.0",
    "react-jss": "8.3.3",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-form": "7.3.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.20.3",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.0.1",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "prettier": "prettier-eslint --write \"src/**/*.+(js|jsx)\"",
    "prettier-dry-run": "prettier-eslint \"src/**/*.+(js|jsx)\" -l debug"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react",
      "@babel/preset-flow",
      "@lingui/babel-preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lingui": {
    "fallbackLocale": "en",
    "format": "lingui",
    "sourceLocale": "",
    "localeDir": "<rootDir>/src/locale",
    "srcPathDirs": [
      "<rootDir>/src"
    ],
    "srcPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
